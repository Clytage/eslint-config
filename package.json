{
    "name": "@clytage-pkg/eslint-config",
    "version": "1.0.2",
    "description": "An ESLint shareable configuration that used in our projects.",
    "main": "index.js",
    "scripts": {
        "lint": "eslint --ignore-path .gitignore .",
        "lint:fix": "eslint --ignore-path .gitignore . --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Clytage/eslint-config.git"
    },
    "keywords": [
        "eslint",
        "eslintconfig",
        "eslint-config",
        "lint",
        "linter",
        "tslint",
        "typescript",
        "typecript-eslint"
    ],
    "author": "Clytage <support@clytage.org>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Clytage/eslint-config/issues"
    },
    "homepage": "https://github.com/Clytage/eslint-config#readme",
    "eslintConfig": {
        "extends": "./index.js"
    },
    "devDependencies": {
        "@types/node": "17.0.33",
        "@typescript-eslint/eslint-plugin": "5.24.0",
        "@typescript-eslint/parser": "5.24.0",
        "eslint": "8.15.0",
        "typescript": "4.6.4"
    },
    "peerDependencies": {
        "@typescript-eslint/eslint-plugin": ">=5.24.0",
        "@typescript-eslint/parser": ">=5.24.0",
        "eslint": ">=8.15.0",
        "typescript": ">=3.3.1 <4.7.0"
    },
    "peerDependenciesMeta": {
        "@typescript-eslint/eslint-plugin": {
            "optional": true
        },
        "@typescript-eslint/parser": {
            "optional": true
        },
        "typescript": {
            "optional": true
        }
    }
}
